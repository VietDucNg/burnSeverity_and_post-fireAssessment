library(readxl)
burnSeverity <- read_excel("output/07_burnSeverity/burnSeverity.xlsx",
sheet = "R_Overall")
View(burnSeverity)
attach(burnSeverity)
# colour setting for the severity map
my_col=c("#FFFFFF","#556b2f","#6e8b3d","#32CD32", "#EEEE00","#ee9a00", "#FF0000", "#800080")
ggplot(data = burnSeverity ,aes(x=`Burnt severity class`,
y= `Area (ha)`))+
geom_bar(stat = "identity", aes(fill= Indices), width = 0.5,
position = "dodge")+
scale_fill_manual("Legend", values = my_col)+
geom_text(label=`Area (ha)`,color="black",size=4,
vjust=-0.2,hjust = 0.5,
position = position_dodge(width= 0.9))+
labs(title="Distribution of the classified class area")+
theme_bw()+
theme(plot.title = element_text(hjust = 0.5,face = "bold"))
dev.off()
